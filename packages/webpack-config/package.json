{
  "name": "ssr-webpack-config",
  "version": "0.1.1",
  "description": "webpack config for serverless ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "scripts": {
    "build": "concurrently \"tsc -p ./tsconfig.cjs.json --sourceMap false\" \" tsc -p ./tsconfig.esm.json --sourceMap false\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.cjs.json \" \"tsc -w -p ./tsconfig.esm.json \"",
    "watch:cjs": "tsc -w -p ./tsconfig.cjs.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykfe/ssr.git"
  },
  "keywords": [
    "webpack",
    "serverless",
    "ssr",
    "react"
  ],
  "author": "zhangyuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ykfe/ssr/issues"
  },
  "homepage": "https://github.com/ykfe/ssr#readme",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "ssr-client-utils": "^0.1.1",
    "ssr-server-utils": "^0.1.1",
    "webpack": "^4.42.1",
    "webpack-chain": "^6.4.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "ssr-types": "^0.1.1",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3"
  }
}
