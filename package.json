{
  "name": "serverless-side-render",
  "version": "1.0.0",
  "description": "implements for serverless side render",
  "directories": {
    "doc": "docs",
    "example": "example"
  },
  "scripts": {
    "build": "lerna run build --stream --parallel",
    "watch": "lerna run watch --stream --parallel",
    "lint": "tslint -c tslint.json -p . && standard",
    "lint:fix": "standard --fix && tslint -c tslint.json -p . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykfe/ssr.git"
  },
  "author": "zhangyuang",
  "license": "MIT",
  "pre-commit": [
    "lint"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": ">=8"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/app/public",
      "**/dist/",
      "**/run/",
      "**/log/",
      "**/lib/",
      "**/bin/",
      "**/es/",
      "packages/ykfe-utils/src/loadable.js",
      "**/node_modules/**",
      "**/output/**",
      "jest.setup.js"
    ],
    "global": [
      "__isBrowser__",
      "location",
      "window",
      "jest"
    ]
  },
  "bugs": {
    "url": "https://github.com/ykfe/ssr/issues"
  },
  "homepage": "https://github.com/ykfe/ssr#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "lerna": "^3.8.0",
    "pre-commit": "^1.2.2",
    "standard": "^12.0.1",
    "tslint": "^5.17.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.7.2"
  }
}
