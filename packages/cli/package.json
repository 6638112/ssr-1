{
  "name": "ssr",
  "version": "1.0.0",
  "description": "cli for serverless ssr",
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "scripts": {
    "build": "concurrently \"tsc -p ./tsconfig.cjs.json --sourceMap false\" \" tsc -p ./tsconfig.esm.json --sourceMap false\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.cjs.json \" \"tsc -w -p ./tsconfig.esm.json \""
  },
  "bin": {
    "ssr": "./cjs/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ykfe/ssr.git"
  },
  "keywords": [
    "cli",
    "serverless",
    "ssr",
    "react"
  ],
  "author": "zhangyuang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ykfe/ssr/issues"
  },
  "homepage": "https://github.com/ykfe/ssr#readme",
  "dependencies": {
    "js-yaml": "^3.13.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.2",
    "@types/yargs": "^13.0.0",
    "concurrently": "^5.1.0"
  }
}
